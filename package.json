    "openai": "^5.10.1",
    "path": "^0.12.7",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.6",
    "twilio": "^5.8.0"
  },
  "devDependencies": {
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.2"
